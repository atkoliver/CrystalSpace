{
    "version": "2.0.0",
    "tasks":
    [
        {
        "label": "Copy Latest Build + Launch Server (1_21_11, Paper)",
        "type": "shell",
        "command": "cmd",
        "args": [
            "/c",
            "start-server.sh",
            "1.21.11",
            "paper"
        ],
        "options": {
            "cwd": "debug-scripts/"
        },
        "presentation": {
            "echo": true,
            "reveal": "always",
            "focus": true,
            "panel": "new"
        },
        "isBackground": true,
        "group": "none",
        "problemMatcher": [
            {
            "pattern": [
                {
                "regexp": ".",
                "file": 1,
                "location": 2,
                "message": 3
                }
            ],
            "background": {
                "activeOnStart": false,
                "beginsPattern": "Starting",
                "endsPattern": "."
            }
            }
        ]
    }
    ]
}
